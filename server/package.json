{
  "name": "nodets-template",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "dotenv -e ../.env.development -- nodemon",
    "build": "tsup ./src/index.ts",
    "seed": "tsx ./src/.scripts/runSeeder.ts",
    "create:router": "tsx ./src/.scripts/createRouter.ts",
    "create:controller": "tsx ./src/.scripts/createController.ts",
    "create:seeder": "tsx ./src/.scripts/createSeeder.ts",
    "insert:controller": "tsx ./src/.scripts/insertController.ts",
    "insert:env": "tsx ./src/.scripts/insertEnv.ts",
    "prepare": "husky",
    "create:env-example": "tsx ./src/.scripts/createEnvExample.ts",
    "migrate:test": "dotenv -e ../.env.test -- npx prisma migrate",
    "migrate:dev": "dotenv -e ../.env.development -- npx prisma migrate",
    "test": "dotenv -e ../.env.test -- npx jest -i"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.3.4",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.820.0",
    "@aws-sdk/s3-request-presigner": "^3.820.0",
    "@prisma/client": "^6.2.1",
    "@types/cookie-parser": "^1.4.8",
    "axios": "^1.9.0",
    "chalk": "^5.4.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "execa": "^9.5.2",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.2.1",
    "supertest": "^7.1.1",
    "zod": "^3.25.32"
  },
  "prisma": {
    "schema": "./src/models"
  }
}
